{
  "name": "workflows",
  "version": "0.1.0",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/app.ts",
    "start.watch": "nodemon src/app.ts",
    "workflow": "ts-node src/client.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@datasert/cronjs-matcher": "^1.2.0",
    "@temporalio/activity": "1.7.0",
    "@temporalio/client": "1.7.0",
    "@temporalio/worker": "1.7.0",
    "@temporalio/workflow": "1.7.0",
    "@tw/types": "^0.4.142",
    "@tw/utils": "^0.7.65",
    "cronstrue": "^2.27.0",
    "nanoid": "3.x"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.0",
    "@types/node": "^16.11.43",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.2"
  }
}
